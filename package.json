{
  "name": "arsam",
  "version": "2.0.0",
  "description": "Arsam Sarabi's portfolio in an interactive terminal dist",
  "entry": "./dist/index.js",
  "files": [
    "lib"
  ],
  "repository": "git@github.com:arsamsarabi/arsam-cli.git",
  "author": {
