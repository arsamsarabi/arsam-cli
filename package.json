  "devDependencies": {
    "@arsams/eslintrc": "^1.0.1",
    "@arsams/prettierrc": "^1.0.2",
    "@types/chalk-animation": "^1.6.1",
    "@types/clear": "^0.1.2",
    "@types/figlet": "^1.5.5",
    "@types/inquirer": "^9.0.3",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
